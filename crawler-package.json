{
  "name": "preipo-sip-crawler",
  "version": "1.0.0",
  "description": "Playwright-based crawler for testing PreIPO SIP application routes",
  "main": "playwright-crawler.ts",
  "scripts": {
    "crawler": "ts-node playwright-crawler.ts",
    "crawler:headless": "HEADLESS=true ts-node playwright-crawler.ts",
    "crawler:screenshots": "SCREENSHOTS=true ts-node playwright-crawler.ts",
    "crawler:full": "SCREENSHOTS=true HEADLESS=false ts-node playwright-crawler.ts",
    "install-browsers": "playwright install chromium"
  },
  "keywords": [
    "playwright",
    "crawler",
    "testing",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.40.1",
    "playwright": "^1.40.1",
    "json2csv": "^6.0.0-alpha.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
